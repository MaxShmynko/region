    &__title-hare
        padding w(50) 0 0 w(190)
        p
            background: conic-gradient(from 180deg at 100% 50%, #ECE61C 0deg, #E3A51C 174.3552589416504deg, #3CC734 346.85837745666504deg);
            background-clip: text;
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            &:last-child
                background: conic-gradient(from 180deg at 23% 40%, #ECE61C 0deg, #E3A51C 174.3552589416504deg, #3CC734 346.85837745666504deg);
                background-clip: text;
                -webkit-background-clip: text;
                -webkit-text-fill-color: transparent;
                padding 0 0 0 w(55)
        +below(720px)
            display flex
            padding 0 0 0 wm(240)

    &__title-elizarov
        background conic-gradient(from 180deg at 19% 80%, #FEC059 0deg, #C23277 360deg)
        background-clip text
        -webkit-background-clip text
        -webkit-text-fill-color transparent
        padding w(60) 0 0 0
        +below(720px)
            padding 0 0 0 wm(190)

    &__title-ravens

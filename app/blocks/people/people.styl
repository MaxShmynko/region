                background-clip: text;
                -webkit-background-clip: text;
                -webkit-text-fill-color: transparent;
                +below(720px)
                    padding 0 0 0 wm(240)

            &:nth-child(2)
                background: conic-gradient(from 180deg at 25% 70%, #391AF5 0deg, #E55D1A 142.47968316078186deg, #3716DD 253.10667514801025deg, #7D2E51 360deg);
                background-clip: text;
                -webkit-background-clip: text;
                -webkit-text-fill-color: transparent;
                padding 0 0 0 w(75)
                +below(720px)
                    padding wm(20) 0 0 0
                    br
                        display none


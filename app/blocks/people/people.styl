        font-size w(64)
        padding w(30)
        +below(720px)
            display none


    &__content
        position relative
        padding w(300) 0 0 w(400)
        +below(720px)
            display flex
            flex-direction column
            align-items center
            padding wm(215) 0 0 wm(45)
        &:nth-child(2n+2)
            padding-left w(530)
            +below(720px)
                padding wm(215) 0 0 wm(45)

        &:nth-child(3)
            +below(720px)
                padding wm(215) 0 0 0

        &:nth-child(4)
            +below(720px)
                padding wm(40) 0 0 0
        &:nth-child(5)
            padding-top w(110)
            +below(720px)
                padding wm(240) 0 0 wm(45)
        &:nth-child(6)
            padding-top w(180)
            +below(720px)
                padding-top wm(220)
        &:nth-child(7)
            padding-left w(300)
            +below(720px)
                padding wm(255) 0 0 wm(45)
        &:nth-child(8)
            padding-left w(720)
            padding-top w(260)
            +below(720px)
                padding wm(160) 0 0 wm(27)
        &:nth-child(9)
            padding-left w(420)
            padding-top w(30)
            +below(720px)
                padding wm(200) 0 0 wm(45)

        

    &__container-title
